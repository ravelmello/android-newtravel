<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ravel.newtravellist" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityPainel"
            android:label="@string/title_activity_activity_painel" >
        </activity>
        <activity
            android:name=".ActivityPanel"
            android:label="@string/title_activity_activity_panel" >
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name=".FindActivity"
            android:label="@string/title_activity_find" >
        </activity>

        <uses-library android:name="com.google.android.maps" />

        <uses-permission android:name="android.permission.INTERNET" />

        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
    </application>

</manifest>
